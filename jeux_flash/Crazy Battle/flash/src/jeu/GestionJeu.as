package jeu {		import flash.display.Sprite;	import flash.events.Event;	import flash.events.MouseEvent;	import jeu.personnages.monsters.Momie;	import jeu.personnages.aliens.Blop;	/**	 * @author Aymeric	 */	public class GestionJeu extends Sprite {				private var conteneurJeu : Sprite=new Sprite();				private var maMomie : Momie;		private var monBlop : Blop;		public function GestionJeu():void {						addChild(conteneurJeu);						creerMomie();						conteneurJeu.addEventListener(Event.ENTER_FRAME, conflit);		}				private function creerMomie():void {						maMomie = new Momie;			conteneurJeu.addChild(maMomie);						monBlop = new Blop;			conteneurJeu.addChild(monBlop);		}				private function conflit(e:Event):void {						if(maMomie.hitTestObject(monBlop)) {				momieVSblop();			}						if (monBlop.hitTestObject(maMomie)) {				blopVSmomie();			}		}				private function momieVSblop():void {			var degatSubit : int = monBlop.force;			maMomie.combat(degatSubit);		}		private function blopVSmomie():void {			var degatSubit : int = maMomie.force;			monBlop.combat(degatSubit);		}	}}