package jeu {		import flash.display.Sprite;	import flash.events.MouseEvent;	import jeu.personnages.monsters.Momie;	/**	 * @author Aymeric	 */	public class GestionJeu extends Sprite {				private var conteneurJeu : Sprite=new Sprite();				private var maMomie : Momie;		public function GestionJeu():void {						addChild(conteneurJeu);		}				private function creerMomie(m:MouseEvent):void {						maMomie = new Momie;			conteneurJeu.addChild(maMomie);		}	}}